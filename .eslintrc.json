{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "quotes": [
            "error",
            "single"
        ],
        "keyword-spacing": "error",
        "indent": [
            "error",
            2
        ]
    }
}