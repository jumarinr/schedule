      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "1.0.5",
    "babel-eslint": "10.1.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-import-resolver-meteor": "0.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
